<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="assignment.swd.toannb.swd_assignment">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HomeActivity"
            android:label="Home"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".EventDetailActionActivity"
            android:label="Event Detail" />
        <activity
            android:name=".CreateEventActivity"
            android:label="Create Event" />
        <activity
            android:name=".CreateMemberActivity"
            android:label="Create Event" />
        <activity
            android:name=".MemberDetailActionActivity"
            android:label="Member Detail" />
        <activity
            android:name=".EventAlbumImageActivity"
            android:label="Album Images" />
        <activity
            android:name=".InventoryActivity"
            android:label="Inventory"/>
        <activity
            android:name=".TransactionActivity"
            android:label="Transaction"/>
    </application>

</manifest>